{
  "extends": ["plugin:prettier/recommended"],
  "plugins": ["react", "prettier"],
  "parser": "babel-eslint",
  "env": {
    "jest": true,
    "browser": true
  },
  "rules": {
    "prettier/prettier": [
      "warn",
      {
        "endOfLine": "auto"
      },
      {
        "printWidth": 300
      }
    ],
    "max-len": "off"
  }
}
